import sys
from PyQt5 import QtWidgets
from stellate.mainwindow import StellateMainWindow


# Main entry point
if __name__ == "__main__":

    app = QtWidgets.QApplication(sys.argv)

    window = StellateMainWindow()
    window.show()

    sys.exit(app.exec_())